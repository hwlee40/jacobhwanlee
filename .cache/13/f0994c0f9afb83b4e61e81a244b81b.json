ved":"/Users/hwan/Desktop/jacobhwanlee/src/images/lex/lex_9.jpg","parent":"/Users/hwan/Desktop/jacobhwanlee/src/lex.html"},{"name":"./images/lex/lex_10.jpg","dynamic":true,"resolved":"/Users/hwan/Desktop/jacobhwanlee/src/images/lex/lex_10.jpg","parent":"/Users/hwan/Desktop/jacobhwanlee/src/lex.html"},{"name":"./images/lex/lex_11.jpg","dynamic":true,"resolved":"/Users/hwan/Desktop/jacobhwanlee/src/images/lex/lex_11.jpg","parent":"/Users/hwan/Desktop/jacobhwanlee/src/lex.html"},{"name":"./images/lex/lex_12.jpg","dynamic":true,"resolved":"/Users/hwan/Desktop/jacobhwanlee/src/images/lex/lex_12.jpg","parent":"/Users/hwan/Desktop/jacobhwanlee/src/lex.html"},{"name":"./images/lex/lex_13.jpg","dynamic":true,"resolved":"/Users/hwan/Desktop/jacobhwanlee/src/images/lex/lex_13.jpg","parent":"/Users/hwan/Desktop/jacobhwanlee/src/lex.html"},{"name":"./images/lex/lex_14.jpg","dynamic":true,"resolved":"/Users/hwan/Desktop/jacobhwanlee/src/images/lex/lex_14.jpg","parent":"/Users/hwan/Desktop/jacobhwanlee/src/lex.html"},{"name":"./images/lex/lex_15.jpg","dynamic":true,"resolved":"/Users/hwan/Desktop/jacobhwanlee/src/images/lex/lex_15.jpg","parent":"/Users/hwan/Desktop/jacobhwanlee/src/lex.html"},{"name":"./images/lex/lex_16.jpg","dynamic":true,"resolved":"/Users/hwan/Desktop/jacobhwanlee/src/images/lex/lex_16.jpg","parent":"/Users/hwan/Desktop/jacobhwanlee/src/lex.html"},{"name":"./images/lex/lex_17.mp4","dynamic":true,"resolved":"/Users/hwan/Desktop/jacobhwanlee/src/images/lex/lex_17.mp4","parent":"/Users/hwan/Desktop/jacobhwanlee/src/lex.html"},{"name":"./images/lex/lex_18.jpg","dynamic":true,"resolved":"/Users/hwan/Desktop/jacobhwanlee/src/images/lex/lex_18.jpg","parent":"/Users/hwan/Desktop/jacobhwanlee/src/lex.html"},{"name":"./images/lex/lex_19.jpg","dynamic":true,"resolved":"/Users/hwan/Desktop/jacobhwanlee/src/images/lex/lex_19.jpg","parent":"/Users/hwan/Desktop/jacobhwanlee/src/lex.html"},{"name":"./images/lex/lex_20.jpg","dynamic":true,"resolved":"/Users/hwan/Desktop/jacobh